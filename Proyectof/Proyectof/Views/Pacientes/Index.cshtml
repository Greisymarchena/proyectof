@model IEnumerable<Proyectof.Models.Pacientes>

@{
    ViewBag.Title = "Index";
}

<h2>Inicio</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cedula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.asegurado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cedula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.asegurado)

        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idPaciente }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idPaciente }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.idPaciente })
        </td>
    </tr>
}

</table>
