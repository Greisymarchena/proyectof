@model IEnumerable<Proyectof.Models.AltaMedica>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Dar una nueva alta", "NewIndex")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fechaIngreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaSalida)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.monto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Habitaciones.idHabitacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ingresos.idIngreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pacientes.cedula)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fechaIngreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaSalida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Habitaciones.idHabitacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ingresos.idIngreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pacientes.cedula)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idAlta }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idAlta }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.idAlta })
        </td>
    </tr>
}

</table>
